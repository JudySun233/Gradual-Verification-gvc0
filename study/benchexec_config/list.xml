<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 2.3//EN" "https://www.sosy-lab.org/benchexec/benchmark-2.3.dtd">
<!-- Example file for benchmark definition for BenchExec. -->
<benchmark tool="tool.gvc0"
           displayName="List"
           memlimit="6GB"
           cpuCores="2"
>
    <!-- <rundefinition> defines a tool configuration to benchmark (can appear multiple times). -->
    <rundefinition name="default">
        <!-- <option> defines command-line arguments (can appear multiple times). -->
        <option name="-b">./benchmarks/list</option>
        <option name="--only-exec"></option>
        <option name="-ws=">8</option>
        <option name="-wu=">128</option>
        <option name="-i=">7</option>
    </rundefinition>
    <!-- <tasks> defines a set of tasks (can appear multiple times). -->
    <tasks name="default-task">
        <includesfile>../../src/test/resources/quant-study/list.c0</includesfile>
    </tasks>
    <resultfiles>./benchmarks/list/</resultfiles>
</benchmark>
